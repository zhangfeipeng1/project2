class=\"mui-card-footer\"></div>\r\n\t\t\t</div>\r\n\r\n\r\n  </div>  \r\n</template>\r\n<style>\r\ntemplate{\r\n  background:#000;\r\n}\r\n</style>\r\n<script>\r\n import {Toast} from \"mint-ui\";\r\n export default {\r\n   data(){\r\n     return {\r\n       name:\"\",\r\n       pwd:\"\"\r\n     }\r\n   },\r\n   methods:{\r\n     handleLogin(){\r\n       //1:获取用户输入 用户名和密码\r\n       var n = this.name;\r\n       var p = this.pwd;\r\n       //2:验证\r\n       var reg = /^[a-z0-9_]{3,12}$/i;\r\n       //3:验证失败提示消息\r\n       if(!reg.test(n)){\r\n         Toast(\"用户名格式不正确\");\r\n         return;\r\n       }\r\n       if(!reg.test(p)){\r\n         Toast(\"密码格式不正确\");\r\n         return;\r\n       }\r\n       //4:发送ajax请求\r\n       //console.log(n+\":\"+p);\r\n       var url = \"http://127.0.0.1:3000\";\r\n       url+=\"/login?name=\"+n;\r\n       url+=\"&pwd=\"+p\r\n       this.axios.get(url).then(result=>{\r\n       //5:如果登录成功 跳转/Home\r\n       //6:如果失败     提示消息\r\n           if(result.data.code == 1){\r\n               Toast(\"登录成功\");\r\n               this.$router.push(\"/Home\")\r\n           }else{\r\n              Toast(\"用户名或密码不匹配\")\r\n           }\r\n       });\r\n     }\r\n   }\r\n }\r\n</script>\r\n<style>\r\n</style>"]}]}