文件\r\n  //import mui from \"../../lib/mui/js/mui.js\";\r\n  export default {\r\n    components:{\r\n      \"swiper-box\":swipe\r\n    },\r\n    created() {\r\n      //console.log(\"list组件参数:\"+this.id);\r\n      this.getImageList();\r\n      this.getGoodsInfo();\r\n    },\r\n    data(){\r\n      return  {\r\n        id:this.$route.params.id,\r\n        list:[],\r\n        val:1,\r\n        info:{}\r\n      }\r\n    },\r\n    methods:{\r\n      getGoodsInfo(){\r\n        //1.获取参数id\r\n        var id=this.id;\r\n         var url=\"http://127.0.0.1:3000/getProduct?id=\"+id;\r\n        //2.发送ajax请求获取商品信息\r\n        this.axios.get(url).then(result=>{\r\n          this.info=result.data.data;\r\n\r\n        })\r\n        //3.保存显示模板\r\n      },\r\n      addCart(){\r\n        //获取参数\r\n        var uid=1;//当前登录用户的id\r\n       var pid=this.id;//商品的编号\r\n        var count=this.val;//商品的数量\r\n        var price=99;\r\n       var url=\"http://127.0.0.1:3000/addCart\"\r\n        //发送ajax请求\r\n       this.axios.get(url,{\r\n         params:{\r\n           uid:uid,\r\n           pid:pid,\r\n           count:count,\r\n           price:price\r\n         }\r\n       }).then(result=>{\r\n         if(result.data.code>0){\r\n           //如果商品添加成功，修改全局共享数据数量\r\n           this.$store.commit(\"increment\");\r\n           Toast(result.data.msg)\r\n         }else{\r\n           Toast(result.data.msg)\r\n         }\r\n       \r\n        })\r\n        //返回结果\r\n        //显示提示消息\r\n      },\r\n      goodsSub(){\r\n        if(this.val > 1){\r\n           this.val--;\r\n        }\r\n      },\r\n      goodsAdd(){\r\n        if(this.val <= 998){\r\n            this.val++;\r\n        }\r\n      },\r\n      getImageList(){\r\n         var url = \"http://127.0.0.1:3000/\";\r\n         url +=\"getImages\";\r\n         this.axios.get(url).then(result=>{\r\n           this.list = result.data;\r\n         });\r\n         //console.log(this.list);?????\r\n       } \r\n    }\r\n  }  \r\n</script>\r\n<style>\r\n</style>"],"sourceRoot":"src\\components\\goods"}]}